<Window x:Class="Games.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Games"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>

        <StackPanel x:Name="TitlePage" VerticalAlignment="Top" HorizontalAlignment="Center" Margin="0,10,0,0">
            <Label Content="ARCADE GAMES" HorizontalAlignment="Center" Margin="0,112,0,0" VerticalAlignment="Top" Height="86" Width="380" FontSize="40" HorizontalContentAlignment="Center"/>
            <Button Content="Whack A Mole" Height="44" Width="380" Margin="0,0,0,20" Click="whackamoleClick"/>
            <Button Content="Quiz Game" Height="44" Width="380" Margin="0,0,0,20" Click="quizMainclick"/>
            <Button Content="Back To Bar" Height="44" Width="380"/>
        </StackPanel>
        <Frame x:Name="ArcadeMain" Margin="0,35,0,0" Navigated="Frame_Navigated" NavigationUIVisibility="Hidden"/>
        

    </Grid>
</Window>
